@model IEnumerable<Category>
    <div class="my-4">
       <h2><span style="color: black;" class="badge bg-warning">Categoreis</span></h2>
        <ul class="list-group">
            @foreach (var cat in Model)
            {
                <li class="list-group-item">
                    @cat.CategoryName
                </li>
            }
        </ul>
    </div>

